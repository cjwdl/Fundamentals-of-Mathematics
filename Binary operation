//移位操作
#include<stdio.h>
int main()
{
  int a = 53;
  int b = a << 1;
  int c = a >> 1;
  printf("%d, %d\n",b,c);
}
//或 与 异或操作
#include<stdio.h>
int main()
{
  int a = 53;
  int b = 35;
  int c = a | b;
  printf("%d\n",c);
   
}

#include<stdio.h>
int main()
{
  int a = 53;
  int b = 35;
  int c = a & b;
  printf("%d\n",c);
   
}
#include<stdio.h>
int main()
{
  int a = 53;
  int b = 35;
  int c = a ^ b;
  printf("%d\n",c);
   
}


